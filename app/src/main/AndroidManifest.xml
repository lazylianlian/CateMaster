<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.catemaster.catemaster">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:name=".base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".base.BaseActivity" />
        <activity android:name=".activitys.SplashActivity" />
        <activity android:name=".activitys.GuideActivity" />
        <activity android:name=".activitys.CateListActivity" />
        <activity android:name=".activitys.CateDetailActivity" />
        <activity android:name=".activitys.LoginAndRegistActivity"></activity>
        </application>

</manifest>